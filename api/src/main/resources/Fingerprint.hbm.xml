<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.ugandaemrfingerprint.model">

	<class name="Fingerprint" table="fingerprint">

        <id name="fingerprintId" type="int" column="fingerprint_id" unsaved-value="0">
            <generator class="native" />
        </id>

		<property name="patient" type="java.lang.String" column="patient"/>

		<property name="finger" type="int" column="finger" />

		<property name="fingerprint" type="java.lang.String" column="fingerprint" />

		<property name="dateCreated" type="java.util.Date" column="date_created"/>
			
	</class>
</hibernate-mapping>
